{
    "name": "alwaysstone/yaml-reader",
    "type": "library",
    "description": "yaml-reader",
    "keywords": [
        "always",
        "yaml",
        "reader",
        "search"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Stefano Sassu",
            "email": "alwasysstone@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~8.1",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~9.5"
    },
    "autoload": {
        "psr-4": {
            "Alwaysstone\\YAMLReader\\": "src/Alwaysstone/YAMLReader"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alwaysstone\\YAMLReader\\": "tests/Alwaysstone/YAMLReader"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
